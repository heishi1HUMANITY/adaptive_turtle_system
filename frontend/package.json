{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^7.1.1",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "chart.js": "^4.4.2",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-zoom": "^2.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.6.2",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "postinstall": "patch-package"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/(?!chartjs-adapter-date-fns|react-chartjs-2|chart.js|@kurkle/color)/",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "overrides": {
    "svgo": "3.3.2",
    "domexception": "4.0.0",
    "rimraf": "5.0.5",
    "nth-check": ">=2.0.1",
    "postcss": ">=8.4.31",
    "@svgr/webpack": {
      "@svgr/plugin-svgo": {
        "svgo": "3.3.2"
      }
    },
    "react-scripts": {
      "@svgr/webpack": {
        "@svgr/plugin-svgo": {
          "svgo": "3.3.2"
        }
      },
      "resolve-url-loader": {
        "postcss": ">=8.4.31"
      },
      "webpack-dev-server": ">=4.15.2"
    },
    "webpack-dev-server": ">=4.15.2"
  },
  "devDependencies": {
    "patch-package": "^8.0.0"
